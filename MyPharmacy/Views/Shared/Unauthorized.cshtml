@{
  ViewBag.Title = "- Unauthorized";
}
<section>
  <h1>
    Unauthorized
  </h1>
</section>
<!-- Main content -->
<section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="row">
            <div class="col-sm-12">
              <div class="text-center error-box">
                <h1 class="bounceInDown animated">Error 401</h1>
                <h2 class="font-xl"><strong><i class="fa fa-fw fa-warning fa-lg text-warning"></i> Unauthorized Access</strong></h2>
                <br>
                <p class="lead">
                  You are not authorized to access the requested page, please contact your administrator. <br>Use your browsers <b>Back</b> button to navigate to the page you have previously come from
                </p>
                <br>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
