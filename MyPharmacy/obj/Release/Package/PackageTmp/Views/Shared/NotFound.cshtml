@{
  ViewBag.Title = "- Page Not Found";
}
<section>
  <h1>
    Page Not Found
  </h1>
</section>
<!-- Main content -->
<section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="row">
            <div class="col-sm-12">
              <div class="text-center error-box">
                <h1 class="bounceInDown animated">Page Not Found 404</h1>
                <h2 class="font-xl"><strong><i class="fa fa-fw fa-times-circle fa-lg text-warning"></i> Sorry, page not found!</strong></h2>
                <br>
                <p class="lead">
                  The page you are looking for does not exist. It may have been moved, or removed altogether.
                </p>
                <br>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section>
