<script type="text/x-kendo-template" id="reasonSelection-template">
  <div id="reasonSelectionForm">
    <div class="panel panel-default">
      <div class="panel-body">
        <div class="row">
          <div class="col-xs-3">
            <h5>Reason</h5>
          </div>
          <div class="col-xs-7" style="margin-top: 5px;">
